{
	"name": "link.test.cn",
	"version": "1.0.0",
	"private": true,
	"author": "kitions\n",
	"scripts": {
		"build": "max build",
		"build:test": "cross-env REACT_APP_ENV=testbuild UMI_ENV=testbuild max build",
		"build:prod": "cross-env REACT_APP_ENV=prod UMI_ENV=prod max build",
		"dev": "rm -rf .umi && max dev",
		"format": "prettier --cache --write .",
		"postinstall": "max setup",
		"lint": "eslint --ignore-path .eslintignore . --fix",
		"lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
		"lint_husky": "eslint --quiet  --ignore-path .eslintignore . ",
		"lint_quiet": "eslint --quiet  --ignore-path .eslintignore . --fix",
		"prepare": "husky install",
		"setup": "max setup",
		"start": "cross-env REACT_APP_ENV=dev UMI_ENV=testbuild npm run dev",
		"start:test": "cross-env REACT_APP_ENV=testbuild UMI_ENV=testbuild max dev",
		"start:prod": "cross-env REACT_APP_ENV=prod UMI_ENV=prod max dev"
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@ant-design/pro-components": "^2.0.1",
		"@ant-design/pro-layout": "7.1.14",
		"@umijs/max": "^4.0.26",
		"antd": "^4.20.7",
		"classnames": "^2.3.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router": "6.3.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@umijs/lint": "^4.0.27",
		"@umijs/preset-ant-design-pro": "^1.3.0",
		"@umijs/preset-react": "^2.1.0",
		"cross-env": "^7.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^2",
		"prettier-plugin-packagejson": "^2",
		"typescript": "^4.5.0"
	},
	"engines": {
		"node": ">=12.0.0"
	}
}
